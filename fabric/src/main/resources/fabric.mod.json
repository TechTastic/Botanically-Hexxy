{
  "schemaVersion": 1,
  "id": "botanically_hexxy",
  "version": "${version}",
  "name": "Botanically Hexxy",
  "description": "A Botania x Hexcasting Interop Addon",
  "authors": [
    "TechTastic"
  ],
  "contact": {
    "homepage": "https://techtastic.github.io/Botanically-Hexxy",
    "sources": "https://github.com/TechTastic/Botanically-Hexxy"
  },
  "license": "MIT",
  "icon": "assets/botanically_hexxy/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.botanically_hexxy.fabric.FabricBotanicallyHexxy"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.botanically_hexxy.fabric.FabricBotanicallyHexxyClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.botanically_hexxy.fabric.FabricBotanicallyHexxyServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.botanically_hexxy.fabric.datagen.FabricBotanicallyHexxyDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.botanically_hexxy.fabric.FabricBotanicallyHexxyModMenu"
    }]
  },
  "mixins": [
    "botanically_hexxy-common.mixins.json",
    "botanically_hexxy-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
